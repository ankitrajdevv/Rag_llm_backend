{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "GOOGLE_API_KEY": "@google_api_key",
    "MONGODB_URI": "@mongodb_uri",
    "MONGODB_DB": "@mongodb_db"
  }
}
